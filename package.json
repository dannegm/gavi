{
    "name": "gavi",
    "version": "3.1.0",
    "private": true,
    "homepage": "./",
    "scripts": {
        "dev": "ESLINT_NO_DEV_ERRORS=true craco start",
        "build": "REACT_APP_EDITOR_MODE=false craco build",
        "build:exportables": "sh ./scripts/buildDownloadables.sh",
        "eject": "react-scripts eject",
        "serve": "yarn build && npx serve ./build",
        "lint": "yarn lint:js && yarn lint:css",
        "lint:js": "eslint . --ext js",
        "lint:css": "stylelint \"**/*.js\"",
        "prettier": "prettier \"**/*.{js,json}\" --check",
        "prettier:fix": "yarn prettier -- --write",
        "make:component": "node ./scripts/makeComponent.js make",
        "exportables:build": "sh ./scripts/buildDownloadables.sh",
        "exportables:upload": "node ./scripts/uploadExportables.js upload"
    },
    "dependencies": {
        "@craco/craco": "^6.2.0",
        "crypto-js": "^4.1.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
        "nanoid": "^3.1.25",
        "papaparse": "^5.3.1",
        "polished": "^4.1.3",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-eva-icons": "^0.0.8",
        "react-helmet": "^6.1.0",
        "react-json-tree": "^0.15.0",
        "react-loadable": "^5.5.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "rsuite": "^4.10.2",
        "slug": "^5.1.0",
        "styled-components": "^5.3.0",
        "styled-normalize": "^8.0.7"
    },
    "devDependencies": {
        "@craco/craco": "^6.2.0",
        "craco-alias": "^3.0.1",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.24.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "firebase": "^9.1.1",
        "firebase-admin": "^9.12.0",
        "husky": "^7.0.1",
        "lint-staged": "^11.1.2",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "stylelint": "^13.13.1",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-config-styled-components": "^0.1.1",
        "stylelint-processor-styled-components": "^1.10.0",
        "yargs": "^17.1.1"
    },
    "lint-staged": {
        "*.{js, jsx, css, json}": [
            "npm run prettier:fix",
            "npm run lint"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
